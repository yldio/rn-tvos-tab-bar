{
  "name": "TvOSExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:tvos": "xcodebuild clean build -workspace ios/TvOSExample.xcworkspace -scheme TvOSExample-tvOS -destination 'platform=tvOS Simulator' CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO",
